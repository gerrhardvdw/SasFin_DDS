          SQLTask:SqlStatementSource="Merge into DimAccount as [Target]&#xA;Using ( Select * from stg_DimAccount) as [Source]&#xA;on ([Target].[FullAccountNr] = [Source].[FullAccountNr] and Target.[Entity] = Source.[Entity])&#xA;&#xA;When Matched &#xA;  And (Target.[ACCATNUM] &lt;&gt; Source.[ACCATNUM]&#xA;  Or Target.[AccountCategoryDescription] &lt;&gt; Source.[AccountCategoryDescription]&#xA;  Or Target.[Entity] &lt;&gt; Source.[Entity]&#xA;&#x9;&#x9;&#xA;&#x9;Or&#x9;Target.[ActivityDescription]  &lt;&gt; Source.[ActivityDescription]&#xA;&#x9;Or&#x9;Target.[DivisionDescription]  &lt;&gt; Source.[DivisionDescription]&#xA;&#x9;Or&#x9;Target.[AreaDescription] &lt;&gt; Source.[AreaDescription]&#xA;&#x9;Or&#x9;Target.[StaffDescription] &lt;&gt; Source.[StaffDescription]&#xA;&#x9;Or&#x9;Target.[CompanyDescription] &lt;&gt; Source.[CompanyDescription]&#xA;&#x9;)&#xA;&#xA;  THEN&#xA;   Update&#xA;     Set Target.[ACCATNUM] = Source.[ACCATNUM],&#xA;&#x9;&#x9;Target.[AccountCategoryDescription] = Source.[AccountCategoryDescription],&#xA;&#x9;&#x9;Target.[Entity] = Source.[Entity],&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;Target.[ActivityDescription]  = Source.[ActivityDescription],&#xA;&#x9;&#x9;Target.[DivisionDescription]  = Source.[DivisionDescription],&#xA;&#x9;&#x9;Target.[AreaDescription]  = Source.[AreaDescription],&#xA;&#x9;&#x9;Target.[StaffDescription]  = Source.[StaffDescription],&#xA;&#x9;&#x9;Target.[CompanyDescription] = Source.[CompanyDescription]&#xA;&#xA;When Not Matched By Target then&#xA; INSERT &#xA;&#x9;&#x9;&#x9;([ACTINDX]&#xA;           ,[AccountNr]&#xA;           ,[SystemAccountNr]&#xA;           ,[Entity]&#xA;           ,[AccountDescr]&#xA;           ,[Division]&#xA;           ,[Activity]&#xA;           ,[Area]&#xA;           ,[Staff]&#xA;           ,[Company]&#xA;           ,[FullAccountNr]&#xA;           ,[StartDate]&#xA;           ,[EndDate]&#xA;           ,[DivisionDescription]&#xA;           ,[ActivityDescription]&#xA;           ,[AreaDescription]&#xA;           ,[StaffDescription]&#xA;           ,[CompanyDescription]&#xA;           ,[PSTNGTYP]&#xA;           ,[ACCATNUM]&#xA;           ,[ACTIVE]&#xA;           ,[ACCTTYPE]&#xA;           ,[PostingTypeDescription]&#xA;           ,[Copy of PostingTypeDescription]&#xA;           ,[AccountCategoryDescription]&#xA;           ,[BS_Category]&#xA;           ,[Stars_AccountDescription]&#xA;           ,[Calypso_AccountDescription]&#xA;           ,[Enterprise_AccountDescription]&#xA;           ,[LeaseWave_AccountDescription])&#xA;&#x9;VALUES&#xA;&#x9;(  Source.[ACTINDX]&#xA;      ,Source.[AccountNr]&#xA;      ,Source.[SystemAccountNr]&#xA;      ,Source.[Entity]&#xA;      ,Source.[AccountDescr]&#xA;      ,Source.[Division]&#xA;      ,Source.[Activity]&#xA;      ,Source.[Area]&#xA;      ,Source.[Staff]&#xA;      ,Source.[Company]&#xA;      ,Source.[FullAccountNr]&#xA;      ,Source.[StartDate]&#xA;      ,Source.[EndDate]&#xA;      ,Source.[DivisionDescription]&#xA;      ,Source.[ActivityDescription]&#xA;      ,Source.[AreaDescription]&#xA;      ,Source.[StaffDescription]&#xA;      ,Source.[CompanyDescription]&#xA;      ,Source.[PSTNGTYP]&#xA;      ,Source.[ACCATNUM]&#xA;      ,Source.[ACTIVE]&#xA;      ,Source.[ACCTTYPE]&#xA;      ,Source.[PostingTypeDescription]&#xA;      ,Source.[Copy of PostingTypeDescription]&#xA;      ,Source.[AccountCategoryDescription]&#xA;      ,Source.[BS_Category]&#xA;&#x9;  ,Source.[Stars_AccountDescription]&#xA;      ,Source.[Calypso_AccountDescription]&#xA;      ,Source.[Enterprise_AccountDescription]&#xA;      ,Source.[LeaseWave_AccountDescription]&#xA;);" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
