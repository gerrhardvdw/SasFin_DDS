                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With CTE_EQ as
(
SELECT 
      [LineNr]
     ,[AccountNr]
  FROM [DDS].[dbo].[BalanceSheetAccounts]
  Group BY 
    [LineNr]
  ,[AccountNr]
  Having [LineNr] &gt;= '80' and [LineNr] &lt;= '110' 
