                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With CTE as
(
SELECT [ACTINDX]
      ,[AccountNr]
      ,Case [Entity]
	    When 'SBANK Dev' then 'SBANK'
		Else [Entity]
		END as [Entity]
      ,[SystemAccountNr]
      ,[AccountDescr]
      ,Trim([Division]) as [Division]
      ,Trim([Activity]) as [Activity]
      ,Trim([Area]) as [Area]
      ,Trim([Staff]) as [Staff]
      ,Trim([Company]) as [Company]
      ,[FullAccountNr]
      ,[StartDate]
      ,[EndDate]
  FROM [dbo].[stg_Accounts]
  )

  Select a.*
       ,b.DSCRIPTN as [DivisionDescription]
	   ,c.DSCRIPTN as [ActivityDescription]
	   ,d.DSCRIPTN as [AreaDescription]
	   ,e.DSCRIPTN as [StaffDescription]
  From CTE a
  Left Join [dbo].[SegmentValues] b 
  on b.[Database] = a.Entity and b.SGMNTID = a.Division and b.SGMTNUMB = 2
   Left Join [dbo].[SegmentValues] c 
  on c.[Database] = a.Entity and c.SGMNTID = a.Activity and c.SGMTNUMB = 3
  Left Join [dbo].[SegmentValues] d 
  on d.[Database] = a.Entity and d.SGMNTID = a.Area and d.SGMTNUMB = 4
  Left Join [dbo].[SegmentValues] e 
  on e.[Database] = a.Entity and Right('000' + Cast(Trim(e.[SGMNTID]) as varchar(3)),3) = a.Staff and e.SGMTNUMB = 5</property>
