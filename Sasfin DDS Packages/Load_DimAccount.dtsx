SELECT [Entity]
      ,[ACTINDX]
      ,[AccountNr]
	  ,Trim([AccountNr]) + '-' + Left(Trim([Division])+'  ',3) + '-' + Trim([Activity]) + '-' + Trim([Area]) + '-' + Trim([Staff]) + '-' + Trim([Company]) as [SystemAccountNr]
