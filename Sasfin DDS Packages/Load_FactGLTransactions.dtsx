              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">With CTE as
(
SELECT [TransactionID]
       
      ,[Entity]
      ,[ACTINDX]
      ,[AccountNr]
      ,[FullAccountNr]
      
	  ,CASE [PeriodID]
	     When '0' Then [Year1] - 1
		 Else [Year1]
		 END as [Year1]
      ,CASE [PeriodID]
	     When '0' Then 12
		 Else [PeriodID]
		 END as [PeriodID]
     
      ,[NET_AMT]
      ,[CRDTAMNT]
      ,[DEBITAMT]
      ,[StartDate]
      ,[EndDate]
	  ,CASE [PeriodID]
	     When '0' Then 'Adjustment'
		 Else 'Normal'
		 END as [TransactionClassification]
  FROM [dbo].[stg_Transactions] 

  )

  Select a.TransactionID
  ,a.AccountNr
  ,a.ACTINDX
  ,a.Entity
  ,a.FullAccountNr
  ,a.PeriodID
  ,a.Year1
  ,a.StartDate
  ,a.EndDate
  ,a.CRDTAMNT
  ,a.DEBITAMT
  ,a.NET_AMT
  ,a.[TransactionClassification]
     ,b.AccountID as [AccountKey]
	 ,c.ID as [DateKey]
  from CTE a


   left join DimAccount b on b.FullAccountNr = a.[FullAccountNr]
   Left Join DimDate c on c.[FiscalYear] = a.[Year1] and c.[FiscalMonth] = a.[PeriodID] and c.[Day] = 1</property>
